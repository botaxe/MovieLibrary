<h2 class="page-header">Edit Movie</h2>
<form action="/movies/<%= movie.id %>?_method=PUT" method="POST">
  <%- include('_form_fields') %>
  <div class="form-row form-row-end btn-row button_container">
    <a class="btn" href="/movies"><span>Cancel</span></a>
    <button class="btn" type="submit"><span>Update</span></button>
  </div>
</form>
